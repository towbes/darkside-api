<Window x:Class="DarksideGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DarksideGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#FF4E4646">
    <StackPanel HorizontalAlignment="Left">
        <Button Command="{Binding Path=SaveCommand}" Width="123" Click="Button_Click">Run Command</Button>
        <Button Command="{Binding Path=InjectCommand}" CommandParameter="25" Width="116" Click="Button_Click_1">Inject</Button>
        <TextBox Name="NumberTextBox"/>
    </StackPanel>
</Window>
