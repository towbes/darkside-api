<Window x:Class="DarksideGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DarksideGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#FF4E4646">
    <StackPanel HorizontalAlignment="Left">
        <Button Command="{Binding Path=SaveCommand}" Width="97">Run Command</Button>
        <Button Width="95">Button 2</Button>
        <Button Width="94">Button 3</Button>
    </StackPanel>
</Window>
